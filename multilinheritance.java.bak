class Addtion
{
	public void add()
	{
		int a=20;
		int b=50;
		int c=a+b;
		System.out.println(c);
	}
}
class Substraction extends Addition
{
	void sub()
	{
		int a=40;
		int b=40;
		int c=a-b;
		System.out.println(c);
	}
}
class Multiplication extends Substraction
{
	void mul()
	{
		int x=40;
		int y=20;
		int z=x*y;
	}
}
class multilinheritance
{
	public static void main(String [] args)
	{
		Multiplication m1=new Multiplication();
		m1.add();
		m1.sub();
		m1.mul();
	}
}

	
	